"webkit" {}

# Method http方法类型 #
Method -> Str

GET     :Method : "GET"
POST    :Method : "POST"
PUT     :Method : "PUT"
DELETE  :Method : "DELETE"
PATCH   :Method : "PATCH"
OPTIONS :Method : "OPTIONS"

# NewMethod Method构建函数 #
NewMethod(v:Str -> r:Method) {
	<- Method(v)
}

me: Method -> {
	String(-> r: Str) {
		<- string(me)
	}
} 

# isMethod 判断是否存在的方法 #
isMethod(m: Method -> r: Bool) {
    b := False
	m ? GET,POST,PUT,DELETE,PATCH,OPTIONS {
        b = True
	}
	<- b
}
